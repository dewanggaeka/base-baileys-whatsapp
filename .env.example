# Kosongkan untuk login via QR
# Isi nomor E.164 TANPA '+' untuk pairing code, contoh: 62812xxxxxxxx
PAIRING_PHONE=

# Biarkan false agar notifikasi tetap muncul di HP
MARK_ONLINE=false

# Sinkron histori penuh (lebih berat). Aktifkan hanya jika butuh.
SYNC_FULL_HISTORY=false

# lokasi file log (folder akan dibuat jika belum ada)
LOG_FILE=logs/bot.log
# level: trace|debug|info|warn|error|fatal
LOG_LEVEL=debug

# kalau pakai QR: atur ke true agar QR muncul di terminal
# kalau kamu pakai pairing code saja, set false
PRINT_QR=true
